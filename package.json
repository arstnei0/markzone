{
	"name": "markzone",
	"scripts": {
		"dev": "solid-start dev",
		"build": "prisma generate && solid-start build && pnpm postbuild",
		"start": "solid-start start",
		"lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
		"lint:fix": "eslint fix \"**/*.{ts,tsx,js,jsx}\"",
		"push": "prisma db push",
		"postinstall": "prisma generate",
		"postbuild": "cp node_modules/.pnpm/**/**/prisma/*engine*.node .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/api.func/",
		"format": "prettier -w ."
	},
	"type": "module",
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"eslint": "^8.31.0",
		"eslint-plugin-solid": "^0.9.1",
		"prisma": "^4.8.1",
		"solid-start-node": "^0.2.11",
		"solid-start-vercel": "^0.2.11",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"dependencies": {
		"@adeora/solid-query": "^0.11.0",
		"@auth/core": "^0.2.5",
		"@auth/solid-start": "^0.1.0",
		"@next-auth/prisma-adapter": "^1.0.5",
		"@prisma/client": "^4.8.1",
		"@solidjs/meta": "^0.28.2",
		"@solidjs/router": "^0.6.0",
		"@stefanprobst/rehype-shiki": "^2.2.0",
		"@tanstack/solid-query": "^4.22.0",
		"@trpc/client": "^10.8.1",
		"@trpc/server": "^10.8.1",
		"keyv": "^4.5.2",
		"keyv-file": "^0.2.0",
		"rehype-stringify": "^9.0.3",
		"remark": "^14.0.2",
		"remark-gfm": "^3.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"remark-toc": "^8.0.1",
		"shiki": "^0.12.1",
		"solid-js": "^1.6.8",
		"solid-start": "^0.2.11",
		"solid-start-trpc": "^0.0.16",
		"solid-trpc": "^0.0.11-rc.1",
		"undici": "5.14.0",
		"unified": "^10.1.2",
		"vite-plugin-top-level-await": "^1.2.2",
		"zod": "^3.20.2"
	},
	"engines": {
		"node": ">=16"
	}
}
